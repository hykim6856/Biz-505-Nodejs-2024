extends ../layout


block content
    link(rel='stylesheet', href='/css/list.css')
    script(src="/js/list.js")
    h1.student  학생정보
    table.student.list
        thead 
            tr
                th Seq
                th 학번
                th 학과
                th 이름
        tbody 
            each item,index in stList
                tr
                    td #{index+1}
                    td= item.st_num
                    td= item.st_name
                    td= item.st_dept
    div.btn_box
        a(href="/student/input") 학생정보추가